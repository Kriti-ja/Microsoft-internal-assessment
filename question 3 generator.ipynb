   "source": [
    "def fibonacci(n):\n",
    "    a, b = 0, 1\n",
    "    while a <= n:\n",
    "        yield a\n",
    "        a, b = b, a + b\n",
    "        \n",
    "for num in fibonacci(int(input(\" enter a no. , up to where you want sequence\"))):\n",
    "    print(num)\n"
   ]
